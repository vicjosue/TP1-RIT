        #include <openssl/rsa.h>

        int RSA_print(BIO *bp, RSA *x, int offset);
        int RSA_print_fp(FILE *fp, RSA *x, int offset);

        #include <openssl/dsa.h>

        int DSAparams_print(BIO *bp, DSA *x);
        int DSAparams_print_fp(FILE *fp, DSA *x);
