       psresize(1), psmerge(1), fixscribeps(1), getafm(1), fixdl­
       srps(1),  fixfmps(1),  fixmacps(1),  fixpsditps(1),  fixp­
       spps(1), fixtpps(1), fixwfwps(1), fixwpps(1),  fixwwps(1),
       extractres(1), includeres(1)

